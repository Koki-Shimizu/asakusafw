"EX1とEX2を結合したモデル"
@auto_projection
@namespace(
    value = view
)
joined ex_joined = ex1 -> {
    "EX1.SID"
    @windgate.column(
        value = "SID1"
    )
    sid -> sid1;
    "EX1.VALUE"
    @windgate.column(
        value = "VALUE"
    )
    value -> value;
} % value + ex2 -> {
    "EX2.SID"
    @windgate.column(
        value = "SID2"
    )
    sid -> sid2;
    "EX2.VALUE"
    @windgate.column(
        value = "VALUE"
    )
    value -> value;
} % value;